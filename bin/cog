#!/bin/bash
CMD=${1:-help}

# Drop next argument
shift

# Check if /bin exist in current directory
if [ -f "./bin/cog-$CMD" ]; then
    echo "[1/3] ðŸ‘€  Found bin folder..."
    # Overwrite script exist. Run this
    echo "[2/3] ðŸ“‚  Changing directory..."
    echo "[3/3] âœ¨  Executing custom script..."
    ( "./bin/cog-$CMD" "${@}")
  else
    # Execute default scripts
    ( "cog-$CMD" "${@}" )
fi
